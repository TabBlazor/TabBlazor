@page "/"
@namespace Tabler.Docs.Server.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Tabler.Docs.Server</title>
    <base href="~/" />

   
    <link rel="stylesheet" href="@("https://unpkg.com/@tabler/core@1.0.0-beta20/dist/css/tabler.min.css")"> 
    
    <link rel="stylesheet" href="_content/TabBlazor/css/tabblazor.min.css" />
    <script src="_content/TabBlazor/js/tabblazor.js"></script>

    <link href="Tabler.Docs.Server.styles.css" rel="stylesheet" />
    <link href="_content/Tabler.Docs/css/tabler-docs.css" rel="stylesheet" />

    <script src="_content/Blazor-ApexCharts/js/apex-charts.min.js"></script>
    <script src="_content/Blazor-ApexCharts/js/blazor-apex-charts.js"></script>


</head>
<body data-bs-theme="">
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
</body>
</html>
